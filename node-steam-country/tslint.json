{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "forin": false,
      "quotemark": [
          true,
          "single"
      ],
      "no-console": [
          false
      ],
      "one-line": [
          true,
          "check-open-brace",
          "check-whitespace"
      ],
      "object-literal-sort-keys": [
          false
      ],
      "no-string-literal": false,
      "triple-equals": [
          false
      ],
      "ordered-imports": [
          false
      ],
      "prefer-for-of": [
          false
      ]
  },
  "rulesDirectory": []
}