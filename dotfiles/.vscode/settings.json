	"[javascript]": {
	  "editor.tabSize": 4,
	  "editor.insertSpaces": true
	},
	"[html]": {
	  "editor.tabSize": 2,
	  "editor.insertSpaces": true
	},
	"[c]": {
	  "editor.tabSize": 8,
	  "editor.insertSpaces": false
	},
	"[markdown]": {
	  "editor.wordWrap": "on",
	  "editor.tabSize": 4,
	  "editor.quickSuggestions": false
	},
	"[typescript]": {
	  "editor.tabSize": 4,
	  "editor.insertSpaces": true
	},
	"extensions.ignoreRecommendations": true,
	"explorer.confirmDragAndDrop": false
